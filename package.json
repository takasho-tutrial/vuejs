    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.1",
    "file-loader": "^1.1.10",
    "flow-bin": "^0.68.0",
    "ghpages": "^0.0.10",
    "node-sass": "^4.7.2",

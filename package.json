    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.47.0",
    "ghpages": "^0.0.10",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
